{
  "name": "vue-spa-webpack-scaffold",
  "version": "1.0.0",
  "description": "vue+webpack搭建的SPA(single page web application)脚手架",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "build:test": "npm run clean && cross-env NODE_ENV=test node build/build.js",
    "build": "npm run clean && node build/build.js",
    "clean": "rm -rf dist && rm -f dist.zip"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
