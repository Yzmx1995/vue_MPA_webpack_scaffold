    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "glob": "^7.1.4",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1"
